{
  "name": "identity-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
     "db": "npx sequelize-cli db:create --config ./sequelizeCli.configs.json",
    "migrate": "npx sequelize-cli db:migrate --config ./sequelizeCli.configs.json --migrations-path ./src/database/migrations",
    "seed": "npx sequelize-cli db:seed:all --config ./sequelizeCli.configs.json --seeders-path src/database/seeders"
  },
